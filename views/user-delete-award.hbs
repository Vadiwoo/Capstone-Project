<h1>Here is your Award History! </h1>

<div class="background">
    <div>
        <table>
            <thead>
            <th>Award ID</th>
            <th>Award Name</th>
            <th>Award Recipient</th>
            <th>Award Created Date</th>
            </thead>

            <tbody>
                {{#each payload}}
                <tr>
                    <td>{{id}}</td>
                    <td>{{name}}</td>
                    <td>{{recipient}}</td>
                    <td>{{date}}</td>
                    <td><button type="submit" class="btn btn-default">DELETE</button></td>


                </tr>
                {{/each}}
                
            </tbody>

        </table>
    </div>

</div>